<UserControl x:Class="PastMediaPlayer_Project.MediaControl.FolderTreeCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PastMediaPlayer_Project.MediaControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TreeView Margin="0,0,0,0">
            <TreeViewItem Name="RootNode"
                Header="Root" MouseRightButtonDown="TreeViewItem_MouseRightButtonDown">
                
            </TreeViewItem>
        </TreeView>
    </Grid>
</UserControl>
